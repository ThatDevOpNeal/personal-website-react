{
  "name": "personal-website-react",
  "version": "1.0.0",
  "description": "personal-website",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "start": "webpack-dev-server --open --mode development",
    "compile:sass": "node-sass sass/main.scss public/static/css/style.css",
    "watch:sass": "node-sass sass/main.scss public/static/css/style.css -w",
    "build": "webpack --mode production",
    "deploy": "webpack --mode production && aws s3 sync dist/ s3://nealbharucha.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": "git@github.com:ThatDevOpNeal/personal-website-react.git",
  "keywords": [
    "react",
    "Neal",
    "Bharucha",
    "Developer",
    "Engineer"
  ],
  "author": "Neal Bharucha <nealbharucha@yahoo.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.11.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "file-loader": "^3.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-particles-js": "^2.4.2",
    "react-router-dom": "^4.3.1",
    "url-loader": "^1.1.2"
  }
}
